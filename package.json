    "@types/html-webpack-plugin": "^3.2.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/speed-measure-webpack-plugin": "^1.3.0",
    "@types/styled-components": "^5.1.0",
    "@types/uuid": "^8.0.0",
