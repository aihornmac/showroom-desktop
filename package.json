{
  "name": "showroom-desktop",
  "productName": "Showroom Desktop",
  "version": "0.0.1",
  "description": "A showroom desktop",
  "bin": "bin.js",
  "main": "main/entry.js",
  "scripts": {
    "start": "electron tools/register.js",
    "build": "ts-node -T --project tsconfig.json tools/build.ts",
    "tsc": "tsc --noEmit -p src/tsconfig.build.json",
    "test": "mocha -r ts-node/register/transpile-only **/__tests__/**/*.test.ts",
    "try": "node -r ts-node/register/transpile-only tools/bin.ts",
    "make": "electron-packager tmp --out dist --overwrite",
    "spike": "node -r ts-node/register/transpile-only tools/spike.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aihornmac/showroom-desktop.git"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "showroom",
    "desktop",
    "gui"
  ],
  "author": "Aihorn Mac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aihornmac/showroom-desktop/issues"
  },
  "homepage": "https://github.com/aihornmac/showroom-desktop#readme",
