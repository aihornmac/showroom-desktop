  "description": "A showroom desktop",
  "bin": "bin.js",
  "scripts": {
    "start": "electron tools/register.js",
    "build": "ts-node -T --project tsconfig.json tools/build.ts",
    "tsc": "tsc --noEmit -p src/tsconfig.build.json",
    "test": "mocha -r ts-node/register/transpile-only **/__tests__/**/*.test.ts",
